#!/bin/bash

read n
if [ "$n" -gt 10 ]
then 
	echo "greater than"
elif [ "$n" -lt 10 ]
then 
	echo "less than"
	
else
	echo "equal to"
	
fi	

if [ "$?" -eq 0 ]
then

echo "do you want to continue? (y/n)"		
read choice
	if [ "$choice" -eq "y" ]
	then echo "no....u can't........bye bye"
	else
	     echo "no problemo!!!...bye bye"
	fi
	
fi	
