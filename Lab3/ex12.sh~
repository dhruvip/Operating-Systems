str='Ar'
search='Ar'
len="${#search}"
for(( i=0 ; i<="$len" ; i++))
do
if [ "${str:$i:1}" = "$search" ]
     then
     	echo "hi" 
 else
 	echo 'no'
fi
done 	    	  	
